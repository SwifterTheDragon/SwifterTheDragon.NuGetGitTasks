<Project>
    <PropertyGroup>
        <_NuGetGitTasks_TaskFolder>$(MSBuildThisFileDirectory)..\tasks\netstandard2.0</_NuGetGitTasks_TaskFolder>
        <CustomTasksAssembly>$(_NuGetGitTasks_TaskFolder)\$(MSBuildThisFileName).dll</CustomTasksAssembly>
    </PropertyGroup>
    <UsingTask TaskName="SwifterTheDragon.NuGetGitTasks.Core.NuGetGitBranchTask" AssemblyFile="$(CustomTasksAssembly)" />
    <PropertyGroup>
        <UnknownHeadLabel Condition="'$(UnknownHeadLabel)' == ''">-UNKNOWN-HEAD-</UnknownHeadLabel>
        <InvalidHeadLabel Condition="'$(InvalidHeadLabel)' == ''">-INVALID-HEAD-</InvalidHeadLabel>
        <DetachedHeadLabel Condition="'$(DetachedHeadLabel)' == ''">-DETACHED-HEAD-</DetachedHeadLabel>
    </PropertyGroup>
</Project>
